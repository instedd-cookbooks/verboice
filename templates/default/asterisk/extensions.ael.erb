context default {
    s => Hangup();
    _ => Hangup();
}

context verboice {
    s => Hangup();
    h => NoOp();
    _. => AGI(agi://localhost:<%= node['verboice']['broker']['agi_port'] %>,,${EXTEN});
}

context verboice-restart {
    h => NoOp();
    _. => AGI(agi://localhost:<%= node['verboice']['broker']['agi_port'] %>,${EXTEN});
}
